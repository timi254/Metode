public class Naloga13 {
    public static int Najdaljsi(String[]tab){
        int poz = 0;
        String najdaljsi="";
        for(int i=0;i<tab.length;i++)
        {
            
            if(tab[i].length()>najdaljsi.length())
            {
                najdaljsi=tab[i];
                poz=i;
            }
        }
        return poz;
    }
    public static void main(String[] args) {
        String[]tab={"jvreijvbireihvb","fuzzrhzhrv","grgrefefffffffffffg","rhejgwuhfhbr","vrhcwnvrunffffffffffffffffb","firjigbhrubnrb"};
        System.out.println(Najdaljsi(tab));
    }
}

import java.util.Scanner;
public class Naloga1 {
    
    public static void pravokotnik(int n, int m){
        for(int i=0;i<=n;i++)
        {
            for(int j=0;j<=m;j++)
            {
                System.out.print("+");
            }
            System.out.println();
        }
    }
    
    public static void main(String[] args) {
        Scanner vhod = new Scanner(System.in);
        pravokotnik(5, 3);
    } 
}
